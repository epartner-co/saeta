{
  "flex-layout.row#container-info__purpose": {
    "props": {
      "blockClass": ["Purpose"]
    },
    "children": ["flex-layout.col#purpose"]
  },
  "flex-layout.col#purpose": {
    "props": {
      "blockClass": ["Purpose__container"]
    },
    "children": ["slider-layout#purpose"]
  },
  "slider-layout#purpose": {
    "props": {
      "blockClass": ["Purpose__slider"],
